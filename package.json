{
  "name": "jest-discord-reporter",
  "version": "1.0.1",
  "description": "Jest reporter used to send test results with Discord.",
  "author": "Jakub Mucha <jakub.mucha@icloud.com>",
  "repository": "git@github.com:drptbl/jest-discord-reporter.git",
  "homepage": "https://github.com/drptbl/jest-discord-reporter",
