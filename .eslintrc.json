{
    "env": {
        "es6": true,
        "node": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off"
    }
}